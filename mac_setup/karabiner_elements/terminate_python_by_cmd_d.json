{
    "description": "Terminate python session by cmd+d",
    "manipulators": [
        {
            "conditions": [
                {
                    "bundle_identifiers": [
                        "^com\\.apple\\.Terminal$"
                    ],
                    "type": "frontmost_application_if"
                }
            ],
            "from": {
                "key_code": "d",
                "modifiers": { "mandatory": ["command"] }
            },
            "to": [
                {
                    "key_code": "d",
                    "modifiers": ["control"]
                }
            ],
            "type": "basic"
        }
    ]
}